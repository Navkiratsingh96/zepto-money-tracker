{
  "manifest_version": 3,
  "name": "Zepto Spending Analyzer",
  "version": "2.0",
  "description": "Track Zepto grocery expenses and analyze top products.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://www.zeptonow.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
